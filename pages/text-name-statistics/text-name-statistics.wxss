.wrap{
  padding: 30rpx;
}

.myTable{
  display: table;
  table-layout: fixed;
  word-wrap: break-word;
  width: 100%;
  border: 1px solid #eee;
  border-radius: 10rpx;
  
}
.myThead{
  display: table-header-group;
}
.myTr{
  display: table-row;
}
.myTbody{
  display: table-row-group;
}
.myTd{
  display: table-cell;
}
.myThead > .myTr > .myTd{
  padding: 8rpx 12rpx;
  border: 1px solid #eee;
  line-height: 60rpx;
  font-size: 12px;
  background: #f5f5f5;
  white-space: nowrap;
}
.myTbody > .myTr > .myTd{
  padding: 8rpx 20rpx;
  border: 1px solid #eee;
  line-height: 60rpx;
  font-size: 12px;
}
.myTbody>.myTr:nth-child(even){
  background: #f5f6f9;
}